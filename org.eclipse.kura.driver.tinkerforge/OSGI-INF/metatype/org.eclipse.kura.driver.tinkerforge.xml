<MetaData xmlns="http://www.osgi.org/xmlns/metatype/v1.2.0" localization="en_us">
    <OCD id="org.eclipse.kura.driver.tinkerforge" 
         name="TinkerforgeDriver" 
         description="A tinkerforge driver">
         
         <AD id="connection.host"
            name="connection.host"
            type="String"
            cardinality="0"
            required="true"
            default="localhost"
            description="Host to connect to over TCP/IP">
         </AD>
         
        <AD id="connection.port"
            name="connection port"
            type="Integer"
            cardinality="0"
            required="false"
            default="4223"
            description="Port to connect to over TCP/IP">
         </AD>
    
    </OCD>
    <Designate pid="org.eclipse.kura.driver.tinkerforge" factoryPid="org.eclipse.kura.driver.tinkerforge">
        <Object ocdref="org.eclipse.kura.driver.tinkerforge"/>
    </Designate>
</MetaData>
